{
  "expo": {
    "name": "eeeasy",
    "slug": "eeeasy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/simpleLogo.png",
    "scheme": "myapp",
    "privacy": "public",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/eeeasy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jp.teamA.app",
      "infoPlist": {
        "UIUserInterfaceStyle": "light",
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/simpleLogo.png",
        "backgroundImage": "./assets/images/simpleLogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/simpleLogo.png"
    },
    "description": ""
  }
}
